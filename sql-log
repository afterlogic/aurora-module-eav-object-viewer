
(SELECT DISTINCT name FROM au_eav_attributes_int as attrs, au_eav_entities as entities
	WHERE entity_type = 'CContact' AND entities.id = attrs.id_entity)
UNION

(SELECT DISTINCT name FROM au_eav_attributes_string as attrs, au_eav_entities as entities
	WHERE entity_type = 'CContact' AND entities.id = attrs.id_entity)
UNION

(SELECT DISTINCT name FROM au_eav_attributes_text as attrs, au_eav_entities as entities
	WHERE entity_type = 'CContact' AND entities.id = attrs.id_entity)
UNION

(SELECT DISTINCT name FROM au_eav_attributes_bool as attrs, au_eav_entities as entities
	WHERE entity_type = 'CContact' AND entities.id = attrs.id_entity)
UNION

(SELECT DISTINCT name FROM au_eav_attributes_datetime as attrs, au_eav_entities as entities
	WHERE entity_type = 'CContact' AND entities.id = attrs.id_entity)


